<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Excel Sales: -->
        <record model="ir.sequence" id="data_sequence_excel_sale_export">
            <field name="name">Excel sale export</field>
            <field name="code">sale.order.excel.export.sequence</field>
            <field name="implementation">no_gap</field>
            <field name="active">1</field>
            <field name="prefix">SALE/</field>
            <field name="suffix"></field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">8</field>
            <field name="use_data_range">1</field>
        </record>
        <!-- Excel Purchase: -->
        <record model="ir.sequence" id="data_sequence_excel_purchase_export">
            <field name="name">Excel purchase export</field>
            <field name="code">purchase.order.excel.export.sequence</field>
            <field name="implementation">no_gap</field>
            <field name="active">1</field>
            <field name="prefix">PURCHASE/</field>
            <field name="suffix"></field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">8</field>
            <field name="use_data_range">1</field>
        </record>

    </data>
</odoo>
